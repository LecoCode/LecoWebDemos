<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
    <servlet-name>demo1LoginServlet</servlet-name>
    <servlet-class>com.leco.Demo1.Servers.loginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>demo2LignUpServlet</servlet-name>
    <servlet-class>com.leco.Demo2.Servers.signUp</servlet-class>
  </servlet>

  <!--servlet-mapping-->
  <servlet-mapping>
    <servlet-name>demo1LoginServlet</servlet-name>
    <url-pattern>/demo1/login</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>demo2LignUpServlet</servlet-name>
    <url-pattern>/demo2/lignup</url-pattern>
  </servlet-mapping>
</web-app>
